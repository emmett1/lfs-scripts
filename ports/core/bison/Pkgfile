name=bison
version=3.7.3
release=1
source=(https://ftp.gnu.org/gnu/$name/$name-$version.tar.xz)

build() {
        cd $name-$version

        ./configure --prefix=/usr
	make -j1
        make -j1 DESTDIR=$PKG install
}