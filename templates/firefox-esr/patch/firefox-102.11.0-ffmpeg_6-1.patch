-            av_log(NULL, AV_LOG_ERROR, "Invalid code %"PRIx32" for %d in "  \
-                   "init_vlc\n", buf[j].code, i);                           \
-            if (buf != localbuf)                                            \
-                av_free(buf);                                               \
-            return AVERROR(EINVAL);                                         \
-        }                                                                   \
